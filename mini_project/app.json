{
  "expo": {
    "name": "FriendFinderBB",
    "description": "You can find your loggedin friends position on a map within a distance you choose. ",
    "slug": "FriendFinderBB",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "android",
      "ios"
    ],
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.BenedikteEva.FriendFinderBB"
    },
    "android": {
      "package": "com.BenedikteEva.FriendFinderBB"
    }
  }
}